{
  "name": "vue3-admin",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "kc:create": "plop --plopfile script/create.js",
    "kc:remove": "plop --plopfile script/remove.js",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write \"./**/*.{html,vue,ts,js,json,md}\""
  },
  "lint-staged": {
    "*.{vue,js,ts}": "eslint --fix"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "mockjs": "^1.1.0",
    "naive-ui": "^2.33.3",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.22",
    "pinia-plugin-persistedstate": "^2.2.0",
    "vue": "^3.2.39",
    "vue-i18n": "9"
  },
  "devDependencies": {
    "@iconify-json/material-symbols": "^1.1.18",
    "@iconify-json/mdi": "^1.1.33",
    "@iconify/vue": "^4.0.0",
    "@intlify/vite-plugin-vue-i18n": "^6.0.1",
    "@types/node": "^18.7.18",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vueuse/core": "^9.2.0",
    "eslint": "^8.23.1",
    "eslint-plugin-vue": "^9.4.0",
    "fast-glob": "^3.2.12",
    "fs-extra": "^10.1.0",
    "plop": "^3.1.1",
    "prettier": "^2.7.1",
    "typescript": "^4.8.3",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-icons": "^0.14.9",
    "unplugin-vue-components": "^0.22.7",
    "unplugin-vue-macros": "^0.11.2",
    "vite": "^3.1.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-pages": "^0.26.0",
    "vite-plugin-removelog": "^0.0.4",
    "vite-plugin-use-modules": "^1.3.1",
    "vite-plugin-vue-meta-layouts": "^0.1.1",
    "vite-plugin-windicss": "^1.8.8",
    "vue-router": "~4.1.5",
    "vue-tsc": "^0.40.13",
    "windicss": "^3.5.6"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "fp-ts",
        "rollup",
        "markdown-it",
        "inferred-types",
        "@types/markdown-it",
        "@rollup/pluginutils"
      ]
    }
  }
}
